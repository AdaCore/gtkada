project GtkAda_Src is
   for Languages use ("C", "Ada");
   for Main use ("make_gtk.adb");
   for Source_Dirs use
     ("./", "./glade/", "./gnome/", "./gtkextra/", "./opengl");
   for Object_Dir use "./lib-obj";

   package Compiler is
      for Default_Switches ("Ada") use ("-gnatwa", "-gnatay");
   end Compiler;

   package IDE is
      for VCS_Kind use "cvs";
      for VCS_Log_Check use "log_check";
      for VCS_File_Check use "ci_check";
   end IDE;

end GtkAda_Src;
