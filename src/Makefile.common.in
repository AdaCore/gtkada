CC = @CC@
FPIC = -fPIC

GNATFLAGS = -O2 -gnatn -gnatwuwl
GNATFLAGS_DEVEL = -O2 -gnatag -gnatwuwewl -g

GNATMAKE = gnatmake
OS_SPECIFIC_LINK_OPTIONS = @OS_SPECIFIC_LINK_OPTIONS@
SO_EXT = @SO_EXT@

CP = cp -p -f
CHMOD = chmod
ARFLAGS = r

RANLIB = @RANLIB@

prefix        = @prefix@
exec_prefix   = $(prefix)
bindir        = $(prefix)/bin
libdir        = $(prefix)/lib
incdir        = $(prefix)/include/gtkada
alidir        = $(incdir)
