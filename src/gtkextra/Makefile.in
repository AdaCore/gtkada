SRC=gtkplot.c gtkplotcanvas.c gtkplotprint.c gtkplotps.c \
  gtkpsfont.c gtkcheckitem.c gtkplotpc.c gtksheet.c gtkitementry.c \
  gtkcombobox.c gtkcolorcombo.c gtkbordercombo.c gtkfontcombo.c

OBJ=${SRC:%.c=%.o}
CC=@CC@
FPIC=@FPIC@
CFLAGS=-O2
ALL_CFLAGS=$(CFLAGS) @GTK_CFLAGS@ $(FPIC)

all: $(OBJ)

%.o: %.c %.h
	$(CC) -c $(ALL_CFLAGS) $<

clean:
	-$(RM) $(OBJ)

distclean: clean
	-$(RM) Makefile
