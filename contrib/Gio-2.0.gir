      <type name="utf8" c:type="gchar*"/>
    </constant>
    <constant name="FILE_ATTRIBUTE_STANDARD_COPY_NAME"
              value="standard::copy-name"
              c:type="G_FILE_ATTRIBUTE_STANDARD_COPY_NAME">
